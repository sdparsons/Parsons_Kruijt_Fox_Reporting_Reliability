}

@article{de_ron_psychological_nodate,
	title = {Psychological Networks in Clinical Populations: A tutorial on the consequences of Berkson's Bias},
	url = {https://osf.io/5t8zw},
	doi = {10.31234/osf.io/5t8zw},
	shorttitle = {Psychological Networks in Clinical Populations},
	abstract = {In clinical research, populations are often selected on the sum-score of diagnostic criteria, i.e. symptoms. Estimating statistical models where a subset of the data is selected based on a function of the analyzed variables introduces Berkson’s bias, which presents a potential threat to the validity of findings in the clinical literature. The aim of the present paper is to investigate the effect of Berkson’s bias on the performance of the two most commonly used psychological network models, the Gaussian Graphical Model ({GGM}) for continuous and ordinal data, and the Ising Model for binary data. In two simulation studies, we test how well the models perform in recovering the true network when estimation is based on a subset of the data typically seen in clinical studies. The selection of data varies with respect to sample size and the cut-off sum-score of the analyzed variables. Recovery performance is established via three metrics: correlation, sensitivity, and specificity. In addition, the density and the amount of spurious negative edges are measured. Simulation parameters are based on a large clinical dataset of depressed patients. Results of both studies indicate that higher cut-off sum-score values are associated with lower performance on all metrics and more spurious negative edges. Our findings provide evidence that Berkson’s bias is a considerable and underappreciated problem in the clinical network literature.},
	author = {de Ron, Jill and Fried, Eiko I and Epskamp, Sacha},
	urldate = {2019-04-03},
	langid = {english},
	note = {00000},
	file = {de Ron et al. - Psychological Networks in Clinical Populations A .pdf:C\:\\Users\\parsonss\\Zotero\\storage\\P9PIDRUY\\de Ron et al. - Psychological Networks in Clinical Populations A .pdf:application/pdf}
}


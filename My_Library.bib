@article{yiend_modifying_2014-1,
	title = {Modifying Interpretation in a Clinically Depressed Sample Using ‘Cognitive Bias Modification-Errors’: A Double Blind Randomised Controlled Trial},
	volume = {38},
	issn = {0147-5916, 1573-2819},
	url = {http://link.springer.com/10.1007/s10608-013-9571-y},
	doi = {10.1007/s10608-013-9571-y},
	shorttitle = {Modifying Interpretation in a Clinically Depressed Sample Using ‘Cognitive Bias Modification-Errors’},
	abstract = {Depression has been widely associated with a cognitive deﬁcit leading to the negative interpretation of ambiguous information. Recently, cognitive bias modiﬁcation ({CBM}) procedures have shown that such negative biases are causally related to emotional vulnerability. However, research using {CBM} has been notably lacking in depression. This is the ﬁrst double blind randomised controlled study investigating the effect of cognitive bias modiﬁcation-errors ({CBM}-errors), on depression and its inﬂuence on mood and resilience to stress. {CBM}-errors is a new form of cognitive bias modiﬁcation for interpretation, which targets the full range of cognitive errors, as well as interpretation biases. Forty clinically depressed participants were randomly allocated to a positive training group or neutral text reading control group. Participants trained to make positive interpretations subsequently interpreted novel ambiguous information in a positive manner compared to controls. The results suggest that a positive cognitive bias can be induced in clinically depressed individuals using a simple computerised intervention. There was little evidence of corresponding beneﬁts in terms of mood or response to stress, suggesting that multiple sessions are likely to be needed to confer symptom related change. A systematic investigation of the optimum number and timing of multiple sessions is now called for.},
	pages = {146--159},
	number = {2},
	journaltitle = {Cognitive Therapy and Research},
	author = {Yiend, Jenny and Lee, Jong-Sun and Tekes, Sinem and Atkins, Louise and Mathews, Andrew and Vrinten, Manouk and Ferragamo, Christian and Shergill, Sukhwinder},
	urldate = {2019-01-21},
	date = {2014-04},
	langid = {english},
	note = {00041},
	file = {Yiend et al. - 2014 - Modifying Interpretation in a Clinically Depressed.pdf:C\:\\Users\\parsonss\\Zotero\\storage\\U6AFJ2EG\\Yiend et al. - 2014 - Modifying Interpretation in a Clinically Depressed.pdf:application/pdf}
}


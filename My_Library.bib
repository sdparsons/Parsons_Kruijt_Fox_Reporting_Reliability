	journaltitle = {{PLoS} {ONE}},
	author = {Menne-Lothmann, Claudia and Viechtbauer, Wolfgang and Höhn, Petra and Kasanova, Zuzana and Haller, Simone P. and Drukker, Marjan and van Os, Jim and Wichers, Marieke and Lau, Jennifer Y. F.},
	editor = {Voracek, Martin},
	urldate = {2019-01-16},
	date = {2014-06-26},
	langid = {english},
	file = {Menne-Lothmann et al. - 2014 - How to Boost Positive Interpretations A Meta-Anal.pdf:C\:\\Users\\parsonss\\Zotero\\storage\\YUWQKPRZ\\Menne-Lothmann et al. - 2014 - How to Boost Positive Interpretations A Meta-Anal.pdf:application/pdf}

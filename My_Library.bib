@article{macleod_cognitive_2012,
	title = {Cognitive Bias Modification Approaches to Anxiety},
	volume = {8},
	issn = {1548-5943, 1548-5951},
	url = {http://www.annualreviews.org/doi/10.1146/annurev-clinpsy-032511-143052},
	doi = {10.1146/annurev-clinpsy-032511-143052},
	abstract = {Clinical anxiety disorders and elevated levels of anxiety vulnerability are characterized by cognitive biases, and this processing selectivity has been implicated in theoretical accounts of these conditions. We review research that has sought to evaluate the causal contributions such biases make to anxiety dysfunction and to therapeutically alleviate anxiety using cognitive-bias modiﬁcation ({CBM}) procedures. After considering the purpose and nature of {CBM} methodologies, we show that variants designed to modify selective attention ({CBM}-A) or interpretation ({CBM}-I) have proven capable of reducing anxiety vulnerability and ameliorating dysfunctional anxiety. In addition to supporting the causal role of cognitive bias in anxiety vulnerability and dysfunction and illuminating the mechanisms that underpin such bias, the ﬁndings suggest that {CBM} procedures may have therapeutic promise within clinical settings. We discuss key issues within this burgeoning ﬁeld of research and suggest future directions {CBM} research should take to maximize its theoretical and applied value.},
	pages = {189--217},
	number = {1},
	journaltitle = {Annual Review of Clinical Psychology},
	author = {{MacLeod}, Colin and Mathews, Andrew},
	urldate = {2019-01-16},
	date = {2012-04-27},
	langid = {english},
	file = {MacLeod and Mathews - 2012 - Cognitive Bias Modification Approaches to Anxiety.pdf:C\:\\Users\\parsonss\\Zotero\\storage\\E8UBWDI6\\MacLeod and Mathews - 2012 - Cognitive Bias Modification Approaches to Anxiety.pdf:application/pdf}
}

@article{macleod_emotion_2011,
	title = {Emotion Regulation and the Cognitive-Experimental Approach to Emotional Dysfunction},
	volume = {3},
	issn = {1754-0739, 1754-0747},
	url = {http://journals.sagepub.com/doi/10.1177/1754073910380970},
	doi = {10.1177/1754073910380970},
	abstract = {Since the 1980s, there has been a steady growth of interest in the psychological mechanisms that regulate normal emotional experience. In this same period, cognitive-experimental researchers have sought to delineate the information processing biases that characterize emotional disorders. Exciting potential synergies exist between these two areas of investigation. In this article, we consider ways in which reciprocal benefits could be gained by the constructive transfer of theoretical ideas and methodological approaches between emotion regulation researchers and cognitive-experimental investigators. We also discuss how recent innovations within each field can profitably impact upon progress within the other. It is concluded that the overlap in conceptual constructs, and the convergence of complementary investigative techniques, between these two research domains, provide opportunities for creative synthesis that could significantly enrich understanding of normal and abnormal emotion across future years.},

	file = {Anvari and Lakens - Using Anchor-Based Methods to Determine the Smalle.pdf:C\:\\Users\\parsonss\\Zotero\\storage\\4XDLMQUZ\\Anvari and Lakens - Using Anchor-Based Methods to Determine the Smalle.pdf:application/pdf}

@article{provencher_complete_2011,
	title = {Complete mental health recovery: bridging mental illness with positive mental health},
	volume = {10},
	issn = {1746-5729},
	url = {http://www.emeraldinsight.com/doi/10.1108/17465721111134556},
	doi = {10.1108/17465721111134556},
	shorttitle = {Complete mental health recovery},
	abstract = {Purpose – The purpose of this paper is to propose that the study, and the promotion, of recovery can be augmented by adopting the model of mental health as a complete state. Design/methodology/approach – A literature review of the last two decades was undertaken and pathways to complete mental health in recovery are proposed. Findings – More work is needed to further develop interventions oriented towards the promotion of positive mental health in recovery, targeting the enhancement of positive emotions towards life and a sense of fulﬁllment in private and social life. Positive mental health also deserves more research attention to assess the full range of recovery outcomes related to the restoration and optimization processes. A better understanding of individual and environmental factors facilitating or hindering the achievement of complete mental health in recovery is warranted as well.},
	pages = {57--69},

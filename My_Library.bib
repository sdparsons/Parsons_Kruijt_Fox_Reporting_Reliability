	author = {Blackwell, Simon E. and Rius-Ottenheim, Nathaly and Schulte-van Maaren, Yvonne W.M. and Carlier, Ingrid V.E. and Middelkoop, Victor D. and Zitman, Frans G. and Spinhoven, Philip and Holmes, Emily A. and Giltay, Erik J.},
	urldate = {2019-01-16},
	date = {2013-03},
	langid = {english},
	file = {Blackwell et al. - 2013 - Optimism and mental imagery A possible cognitive .pdf:C\:\\Users\\parsonss\\Zotero\\storage\\NL7EJIKP\\Blackwell et al. - 2013 - Optimism and mental imagery A possible cognitive .pdf:application/pdf}
}

@article{blanchard_risk_2011,
	title = {Risk assessment as an evolved threat detection and analysis process},
	volume = {35},
	issn = {01497634},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S0149763410001806},
	doi = {10.1016/j.neubiorev.2010.10.016},
	abstract = {Risk assessment is a pattern of activities involved in detection and analysis of threat stimuli and the situations in which the threat is encountered. It is a core process in the choice of speciﬁc defenses, such as ﬂight, freezing, defensive threat and defensive attack, that counter the threat and minimize the danger it poses. This highly adaptive process takes into account important characteristics, such as type and location (including distance from the subject) of the threat, as well as those (e.g. presence of an escape route or hiding place) of the situation, combining them to predict which speciﬁc defense is optimal with that particular combination of threat and situation. Risk assessment is particularly associated with ambiguity either of the threat stimulus or of the outcome of available defensive behaviors. It is also crucial in determining that threat is no longer present, permitting a return to normal, nondefensive behavior. Although risk assessment has been described in detail in rodents, it is also a feature of human defensive behavior, particularly in association with ambiguity. Rumination may be a speciﬁcally human form of risk assessment, more often expressed by women, and highly associated with anxiety.},
	pages = {991--998},
	number = {4},
	journaltitle = {Neuroscience \& Biobehavioral Reviews},
	author = {Blanchard, D. Caroline and Griebel, Guy and Pobbe, Roger and Blanchard, Robert J.},
	urldate = {2019-01-16},
	date = {2011-03},
	langid = {english},
	file = {Blanchard et al. - 2011 - Risk assessment as an evolved threat detection and.pdf:C\:\\Users\\parsonss\\Zotero\\storage\\UT6T3C5L\\Blanchard et al. - 2011 - Risk assessment as an evolved threat detection and.pdf:application/pdf}

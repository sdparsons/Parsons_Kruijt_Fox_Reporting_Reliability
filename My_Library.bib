	langid = {english},
	file = {Garland et al. - 2010 - Upward spirals of positive emotions counter downwa.pdf:C\:\\Users\\parsonss\\Zotero\\storage\\MY7L4FJQ\\Garland et al. - 2010 - Upward spirals of positive emotions counter downwa.pdf:application/pdf}

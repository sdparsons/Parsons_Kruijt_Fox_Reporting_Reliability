	number = {2},
	journaltitle = {Journal of Behavior Therapy and Experimental Psychiatry},
	author = {Vanderhasselt, Marie-Anne and Koster, Ernst H.W. and Onraedt, Thomas and Bruyneel, Lynn and Goubert, Liesbet and De Raedt, Rudi},
